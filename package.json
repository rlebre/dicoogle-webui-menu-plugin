{
  "name": "webui-react-template",
  "version": "0.1.0",
  "description": "Template to develop menu web-uiplugins for dicoogle using React",
  "main": "module.js",
  "files": [
    "module.js"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-debug": "webpack --config webpack.dev.js",
    "build-watch": "webpack --config webpack.dev.js --watch",
    "prepare": "npm run build"
  },
  "author": "Rui Lebre <ruilebre@ua.pt>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "rlebre/webui-react-template"
  },
  "keywords": [
    "dicoogle",
    "dicoogle-plugin"
  ],
  "dicoogle": {
    "slot-id": "menu",
    "caption": "React Template",
    "module-file": "module.js"
  },
  "devDependencies": {
    "babel-minify-webpack-plugin": "^0.3.1",
    "file-loader": "^3.0.1",
    "husky": "=4",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "ts-loader": "^4.5.0",
    "typescript": "^2.9.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/react": "^15",
    "@types/react-dom": "^15",
    "dicoogle-client": "^4.2.0",
    "react": "^15",
    "react-dom": "^15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,scss,css,md}": "prettier --write"
  }
}